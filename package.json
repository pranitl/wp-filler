{
  "name": "wp-filler",
  "version": "1.0.0",
  "description": "WordPress Landing Page Automation using Playwright",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node tests/test-automation.js",
    "test:phases": "node tests/test-phases.js",
    "test:stealth": "node tests/test-phases-stealth.js",
    "test:ultra": "node tests/test-ultra-stealth.js",
    "test:diagnose": "node tests/diagnose-login.js",
    "test:clear": "node tests/test-phases-stealth.js clear",
    "docker:build": "docker build -t wp-filler .",
    "docker:run": "docker run -p 3000:3000 --env-file .env wp-filler",
    "docker:dev": "docker run -p 3000:3000 --env-file .env -e HEADLESS=false wp-filler"
  },
  "keywords": [
    "wordpress",
    "automation",
    "playwright",
    "landing-pages"
  ],
  "author": "pranitl",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "playwright": "^1.55.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
